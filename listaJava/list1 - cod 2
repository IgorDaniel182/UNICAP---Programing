//Escreva um programa que leia os valores A, B, C e imprima na tela se a soma de A + B é menor que C.
programa {
  funcao inicio() {
      real A,B,C
      leia(A)
      leia(B)
      leia(C)


      se(A+B < C){
        escreva("A soma de A com B é menor que C")
      }senao{
        escreva("A soma de A com B é maior que C")
      }      
  }
}
