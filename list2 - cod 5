//Escreva uma programa para ler dois valores e imprimir uma das três mensagens a seguir:
//• Números iguais, caso os números sejam iguais;
//• Primeiro é maior, caso o primeiro seja maior que o segundo;
//• Segundo é maior, caso o segundo seja maior que o primeiro.programa

programa
{
	
	funcao inicio()
	{
		
real x,y
		leia(x)
		leia(y)

		se(x == y){
			escreva("Números são iguais!")
		}senao se(x > y){
			escreva("O primeiro é maior que o segundo!")
		}senao se(y < y){
			escreva("O segundo é maior que o segundo!")
		}
	}
}
